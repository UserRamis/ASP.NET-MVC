@model IEnumerable<building_materials_store.Models.ApplicationType>


<div class="container p-3">
    <div class=" row pt-4">
        <div class="col-6 ">
            <h2 class=" text-primary">Category List</h2>
        </div>
        <div class="col-6 text-right">
            <a asp-action="Create" class="btn-primary">
            Create new ApplicationType
            </a>
        </div>
    </div>
    <br /><br />

    @if(Model.Count()>0)
    {
        <table class=" table table-bordered table-striped" style="width100%">
            <thead>
                <tr>
                    <th>
                        Category Name
                    </th>
                    
                    <th>

                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach(var obj in Model)
                {
                    <tr>
                        <td width="50%">@obj.Name</td>
                        
                        <td >r</td>
                    </tr>
                }
            </tbody>
        </table>
    }
    else
    {
        <p>No Applicationtype.</p>
    }



</div>
